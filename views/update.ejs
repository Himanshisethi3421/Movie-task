<body>
    <div id="update">
        <div id="update1">
            <h1>Update...</h1>
        <form action="/update/<%=user._id %>" method="post">
            <div class="mb-3">
              <label id="a" for="exampleInputEmail1" class="form-label">Movie Title..</label>
              <input type="text" name="Title" value="<%=user.Title %>" class="form-control" id="exampleInputEmail1" aria-describedby="emailHelp">
            </div>
            <div class="mb-3">
              <label  id="a" for="exampleInputPassword1" class="form-label">Movie desc..</label>
              <input type="text" name="desc" value="<%=user.desc%>" class="form-control" id="exampleInputPassword1">
            </div>
            <button id="b"type="submit" class="btn btn-dark">Submit</button>
          </form>
        </div>
    </div>

    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.1.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-ka7Sk0Gln4gmtz2MlQnikT1wXgYsOg+OMhuP+IlRH9sENBO0LRn5q+8nbTov4+1p" crossorigin="anonymous"></script>
</body>
